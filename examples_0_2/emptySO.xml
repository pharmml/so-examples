<?xml version="1.0" encoding="UTF-8"?>
<SO xmlns="http://www.pharmml.org/so/0.2/StandardisedOutput"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ds="http://www.pharmml.org/pharmml/0.6/Dataset"
    xmlns:ct="http://www.pharmml.org/pharmml/0.6/CommonTypes"
    xsi:schemaLocation="http://www.pharmml.org/so/0.2/StandardisedOutput 
    http://www.pharmml.org/so/0.2/StandardisedOutput" 
    implementedBy="MJS" writtenVersion="0.2"  
    metadataFile="warfarin_PK_ODE_SO_FULL.rdf" id="i1">
    
    <PharmMLRef name="PharmML_ModelFile.xml"/>
    
    <SOBlock blkId="SO1">
        
        <ToolSettings>...</ToolSettings>
        
        <RawResults>...</RawResults>
        
        <TaskInformation>...</TaskInformation>
        
        <Estimation>
            <PopulationEstimates>...</PopulationEstimates>
            
            <PrecisionPopulationEstimates>...</PrecisionPopulationEstimates>
            
            <IndividualEstimates>...</IndividualEstimates>
            
            <PrecisionIndividualEstimates>...</PrecisionIndividualEstimates>
            
            <Residuals>...</Residuals>
            
            <Predictions>...</Predictions>
            
            <Likelihood>...</Likelihood>
        </Estimation>


        <!-- MODEL DIAGNOSTIC -->
        <ModelDiagnostic>
            <DiagnosticPlotsStructuralModel>...</DiagnosticPlotsStructuralModel>

            <DiagnosticPlotsIndividualParams>...</DiagnosticPlotsIndividualParams>
        </ModelDiagnostic>
        
        <!-- SIMULATION -->
        <Simulation>
            <SimulationBlock replicate="1">         <!-- OPTIONAL: if only one block required -->

                <SimulatedProfiles>...</SimulatedProfiles>

                <IndivParameters>...</IndivParameters>

                <RandomEffects>...</RandomEffects>

                <Covariates>...</Covariates>

                <PopulationParameters>...</PopulationParameters>

                <Dosing>...</Dosing>
            </SimulationBlock>
            <!--            <SimulationBlock replicate="2">
                ...
            </SimulationBlock>-->
        </Simulation>
        
        <OptimalDesign type="evaluation">
            <OptimalDesignBlock blockNumber="1">
                <FIM>...</FIM>
                
                <CovarianceMatrix>...</CovarianceMatrix>
                
                <ParameterPrecision>...</ParameterPrecision>
                
                <Criteria>...</Criteria>
                
                <Tests>...</Tests>
                
                <SimulatedData oid="SimDataOid">...</SimulatedData>
                
                <Design oid="desingOid">...</Design>
                
            </OptimalDesignBlock>
            <!--            <OptimalDesignBlock blockNumber="2">
                ...
            </OptimalDesignBlock>-->
    </SOBlock>
    <!-- more blocks possible -->
</SO>
